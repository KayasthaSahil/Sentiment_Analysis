{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "vercel-python@3.9",
      "excludeFiles": "{*.cache,__pycache__,*.pyc,*.pyo,*.pyd,tests,notebooks,examples}/**"
    }
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
} 